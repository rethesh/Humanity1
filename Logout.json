{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://accounts.humanity.com/"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//input[@id='login-email']"
      },
      "text": "ryanpaulfyfe+testhumanityemployee@gmail.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//label[text()='Password']/following-sibling::input"
      },
      "text": "123123"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//input[@value='Sign in']"
      }
    },
    {
      "type": "pause",
      "waitTime": "20000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//a[@id='userMenuButton']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//a[text()='Logout']"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}